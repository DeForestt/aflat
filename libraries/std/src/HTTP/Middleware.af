import string from "String";

class Middleware {
    const any<Server, HTTPRequest> implementation = implementation;
    
    Middleware init(const any<Server, HTTPRequest> implementation, * const Server server) {
        if server != NULL
            server.addMiddleware(my);
        return my;
    };

